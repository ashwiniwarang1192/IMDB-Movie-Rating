{
  "name": "pretr-partner-api",
  "version": "0.0.1",
  "description": "Pretr Partner APIs",
  "main": "index.js",
  "repository": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/index.ts",
    "copy-config": "cp -Rv ./src/config ./dist",
    "dev": "DEBUG=info nodemon -e ts -w ./src -x npm run watch:serve",
    "watch:serve": "ts-node src/index.ts",
    "test": "export NODE_ENV=test|| set \"NODE_ENV=test\" && export NODE_CONFIG_DIR=\"src/config\" || set  \"NODE_CONFIG_DIR=\"src/config\"\" && mocha --exit",
    "test-cover": "nyc npm run test && nyc --reporter=lcov --report-dir=.nyc_output report",
    "test:watch": "mocha --watch spec --compilers ts:ts-node/register 'test/**/*.test.ts' && gulp watch",
    "tsfiles": "git ls-files | grep '.ts$'",
    "lint": "npm run tsfiles | xargs tslint",
    "lint:fix": "npm run tsfiles | xargs tslint --fix",
    "prettier": "code-utils/prettier.sh",
    "prettier:diff": "git diff --name-only | grep '.ts$' | xargs prettier --print-width 80 --single-quote --no-semi --no-bracket-spacing --write",
    "build": "tsc -p tsconfig-prod.json",
    "build-dev": "tsc -p .",
    "build-test": "npm run build-dev && nyc npm run test",
    "precommit": "lint-staged",
    "test-report": "nyc npm run test && nyc --reporter=lcov --report-dir=.nyc_output report"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@myntra/airbus_node_sdk": "1.1.3",
    "@myntra/node-logger": "0.0.6",
    "@myntra/queue_handler": "1.1.50",
    "@types/node": "18.11.18",
    "agentkeepalive": "4.3.0",
    "axios": "1.2.4",
    "body-parser": "1.20.1",
    "cacheable-lookup": "6.1.0",
    "config": "3.3.9",
    "eslint-config-airbnb": "19.0.4",
    "express": "4.18.2",
    "express-http-context": "1.2.4",
    "express-rate-limit": "6.7.0",
    "file-type": "16.5.4",
    "form-data": "4.0.0",
    "helmet": "6.0.1",
    "ioredis": "5.3.0",
    "joi": "17.7.0",
    "jsonwebtoken": "9.0.0",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "moment-timezone": "0.5.40",
    "mongoose": "6.8.4",
    "mongoose-unique-validator": "3.1.0",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "mysql2": "^3.1.0",
    "newrelic": "9.8.0",
    "node-gyp": "9.3.1",
    "pm2-cluster-prom": "2.1.0",
    "pretrCommonLibraries": "5.3.6-beta.6",
    "reflect-metadata": "0.1.13",
    "sequelize": "6.28.0",
    "sequelize-typescript": "2.1.5",
    "supertest": "6.3.3",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "4.6.0",
    "tsyringe": "4.7.0",
    "typescript": "4.9.4",
    "uuid": "9.0.0",
    "winston": "3.8.2",
    "winston-daily-rotate-file": "4.7.1"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/chai": "4.3.4",
    "@types/config": "3.3.0",
    "@types/express": "4.17.13",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "10.0.1",
    "@types/morgan": "^1.9.4",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "8.31.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "mocha": "10.2.0",
    "node-mocks-http": "1.12.1",
    "nodemon": "2.0.19",
    "nyc": "15.1.0",
    "prettier": "2.5.1",
    "sinon": "15.0.1",
    "sonar-scanner": "^3.1.0",
    "sonarqube-scanner": "^3.0.0",
    "ts-node": "10.9.1"
  },
  "lint-staged": {
    "*.ts": "npx eslint src/**/*.ts --max-warnings=6565 --fix"
  },
  "eslintIgnore": [
    "src/**/*.test.ts"
  ]
}
